function canmeng()
Menu = gg.choice({
'üíã‰∏ÄÈîÆMXÂä†ÂØÜüíã',
'üíã ‰∏ÄÈîÆXÂä†ÂØÜüíã',
'üíã‰∏ÄÈîÆYIC‚ë†Âä†ÂØÜüíã',
'üíã‰∏ÄÈîÆYIC‚ë°Âä†ÂØÜüíã',
'üíã‰∏ÄÈîÆDZSHÂä†ÂØÜüíã',
'üíã‰∏ÄÈîÆDZSH‰∫îÊ¨°Âä†ÂØÜüíã',
'üíã‰∏ÄÈîÆEncÂä†ÂØÜüíã',
'üíã‰∏ÄÈîÆLua+Âä†ÂØÜüíã',
'üíã‰∏ÄÈîÆÊï∞Â≠óÂä†ÂØÜüíã',
'üíã‰∏ÄÈîÆÂéüÂàõBÂä†ÂØÜüíã',
'üíã‰∏ÄÈîÆnanzhiÂä†ÂØÜüíã',
'ÈÄÄÂá∫Á®ãÂ∫è'},nil,'\nËØ∑ÊääËÑöÊú¨ÊîæÂà∞tencent/QQfile_recv/ÁöÑÊñá‰ª∂Â§πÈáåÈù¢Âπ∂‰∏îÊîπÂêç‰∏∫1.lua<Âä†ÂØÜÊàêÂäü‰∏ç‰ºöÊèêÁ§∫ÔºåËØ∑Ëá™Ë°åÂà∑Êñ∞Êñá‰ª∂Â§πÂç≥ÂèØÁúãÂà∞Âä†ÂØÜÂÆåÊàêËÑöÊú¨>')
if Menu == 1 then HZJIA() end
if Menu == 2 then b() end
if Menu == 3 then c() end
if Menu == 4 then d() end
if Menu == 5 then e() end
if Menu == 6 then f() end
if Menu == 7 then g() end
if Menu == 8 then h() end
if Menu == 9 then i() end
if Menu == 10 then j() end
if Menu == 11 then k() end
if Menu == 12 then print('ÈÄÄÂá∫'..sj)os.exit() end
PD1 = 0
end

function getByte(data, flag)
    local array = {}
    local lens = string.len(data)
    if (flag == false)
    then
        for i=1,lens do
            array[i] = string.byte(data, i)
        end
        return array
    else
        for i=1,lens do
            array[i-1] = string.byte(data, i)
        end
    end
    return array,lens
end

function getChars(bytes)
    local array = {}
    for key, val in pairs(bytes) do
        array[key] = string.char(val)
    end
    return array
end

function string.split( str,reps )
    local resultStrList = {}
    string.gsub(str,'[^'..reps..']+',function ( w )
        table.insert(resultStrList,w)
    end)
    return resultStrList
end

function HZJIA()
Path = gg.prompt({[1]='ÈÄâÊã©Ë¶ÅÂä†ÂØÜÁöÑËÑöÊú¨ Êù•Ëá™CMÂä†ÂØÜÁöÑÊèêÁ§∫'}, {[1]='/storage/emulated/0/CMÂä†ÂØÜ/'},{[1]='file'})
if Path==nil then
DZSH() 
else
HZJIA2()
end
end
function HZJIA2()
local file = io.open(Path[1], 'r')
assert(file)
local key = gg.prompt({[1]='ËØ∑ËæìÂÖ•Âä†ÂØÜÂØÜÁ†Å Êù•ÁùÄCMÂä†ÂØÜÁöÑÊèêÁ§∫'}, {[1]=''}, {[1]='text'})

function encryptData(data, keys)
    local result = ''
    local dataArr = getByte(data, false)
    local keyArr,keyLen = getByte(keys, true)
    for index,value in pairs(dataArr) do
        result = result..'B'..tostring((0xFF and value) + (0xFF and keyArr[(index-1) % keyLen]))
    end
jmdm = result
decryption = "--CMÂä†ÂØÜ\n--\n--QQÔºö52831274\n--¬© 20182019 canmeng.\n\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\nLJ=gg.getFile()\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\nfile = io.open(LJ, 'r')assert(file)\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --