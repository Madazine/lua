--\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\nlocal keyArr,keyLen = getByte(keys, true)\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\nfor index,value in pairs(dataArr) do\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\nbytes =  tonumber(value) - (0xFF and keyArr[(index-1) % keyLen])\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\n --\nresult = resul